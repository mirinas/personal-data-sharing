{
  "name": "fablo-test-network-org4",
  "description": "Connection profile for Hyperledger Explorer in Fablo network",
  "version": "1.0.0",
  "client": {
    "organization": "Org4",
    "tlsEnable": true,
    "enableAuthentication": true,
    "adminCredential": {
      "id": "admin",
      "password": "adminpw"
    },
    "connection": {
      "timeout": {
        "peer": {
          "endorser": "300"
        },
        "orderer": "300"
      }
    }
  },
  "organizations": {
    "Org4": {
      "mspid": "Org4MSP",
      "adminPrivateKey": {
        "path": "/tmp/crypto/peerOrganizations/owners.org/users/Admin@owners.org/msp/keystore/priv-key.pem"
      },
      "peers": [
        "peer0.owners.org"
      ],
      "signedCert": {
        "path": "/tmp/crypto/peerOrganizations/owners.org/users/Admin@owners.org/msp/signcerts/Admin@owners.org-cert.pem"
      }
    }
  },
  "peers": {
    "peer0.owners.org": {
      "url": "grpcs://peer0.owners.org:7101",
      "tlsCACerts": {
        "path": "/tmp/crypto/peerOrganizations/owners.org/msp/tlscacerts/tlsca.owners.org-cert.pem"
      }
    }
  },
  "channels": {
    "ch1": {
      "peers": {}
    }
  }
}
