{
  "name": "fablo-test-network-org2",
  "description": "Connection profile for Hyperledger Explorer in Fablo network",
  "version": "1.0.0",
  "client": {
    "organization": "Org2",
    "tlsEnable": true,
    "enableAuthentication": true,
    "adminCredential": {
      "id": "admin",
      "password": "adminpw"
    },
    "connection": {
      "timeout": {
        "peer": {
          "endorser": "300"
        },
        "orderer": "300"
      }
    }
  },
  "organizations": {
    "Org2": {
      "mspid": "Org2MSP",
      "adminPrivateKey": {
        "path": "/tmp/crypto/peerOrganizations/org2.ac/users/Admin@org2.ac/msp/keystore/priv-key.pem"
      },
      "peers": [
        "peer0.org2.ac"
      ],
      "signedCert": {
        "path": "/tmp/crypto/peerOrganizations/org2.ac/users/Admin@org2.ac/msp/signcerts/Admin@org2.ac-cert.pem"
      }
    }
  },
  "peers": {
    "peer0.org2.ac": {
      "url": "grpcs://peer0.org2.ac:7061",
      "tlsCACerts": {
        "path": "/tmp/crypto/peerOrganizations/org2.ac/msp/tlscacerts/tlsca.org2.ac-cert.pem"
      }
    }
  },
  "channels": {
    "ch1": {
      "peers": {}
    }
  }
}
