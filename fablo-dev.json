{
  "$schema": "https://github.com/hyperledger-labs/fablo/releases/download/1.2.1-unstable.0/schema.json",
  "global": {
    "fabricVersion": "2.5.0",
    "tls": false,
    "peerDevMode": true,
    "engine": "docker",
    "tools": {
      "explorer": true
    }
  },
  "orgs": [
    {
      "organization": {
        "name": "Orderer",
        "domain": "orderer.org"
      },
      "orderers": [
        {
	  "groupName": "auth",
          "prefix": "orderer",
          "type": "solo",
          "instances": 1
        }
      ]
    },
    {
      "organization": {
        "name": "Org1",
        "mspName": "Org1MSP",
        "domain": "org1.co"
      },
      "ca": {
        "prefix": "ca"
      },
      "peer": {
        "prefix": "peer",
        "instances": 1,
        "db": "LevelDb"
      },
      "tools": {
        "fabloRest": true
      }
    },
    {
      "organization": {
        "name": "Org2",
        "mspName": "Org2MSP",
        "domain": "org2.ac"
      },
      "ca": {
        "prefix": "ca"
      },
      "peer": {
        "prefix": "peer",
        "instances": 1,
        "db": "LevelDb"
      },
      "tools": {
        "fabloRest": true
      }
    },
    {
      "organization": {
        "name": "Owners",
        "mspName": "OwnersMSP",
        "domain": "owners.org"
      },
      "ca": {
        "prefix": "ca"
      },
      "peer": {
        "prefix": "peer",
        "instances": 1,
        "db": "LevelDb"
      },
      "tools": {
        "fabloRest": true
      }
    }
  ],
  "channels": [
    {
      "name": "ch1",
      "orgs": [
        {
          "name": "Org1",
          "peers": [
            "peer0"
          ]
        },
        {
          "name": "Org2",
          "peers": [
            "peer0"
          ]
        },
        {
          "name": "Owners",
          "peers": [
            "peer0"
          ]
        }
      ]
    }
  ],
  "chaincodes": [
    {
      "name": "private-data",
      "version": "1.0",
      "lang": "java",
      "channel": "ch1",
      "directory": "private-data-chaincode",
      "endorsement": "OR('OwnersMSP.member', 'Org1MSP.member', 'Org2MSP.member')",
      "privateData": [
        {
          "name": "CollectionA",
          "orgNames": [
            "Org1",
            "Org2"
          ]
        },
        {
          "name": "CollectionB",
          "orgNames": [
            "Org1",
	    "Owners"
          ]
        }
      ]
    }
  ]
}
